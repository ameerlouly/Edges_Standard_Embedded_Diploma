#include <stdio.h>
#include <stdlib.h>

long math(int x, int y, char c)
{
	switch(c)
	{
	case '+':
		return x + y;
		break;
	case '-':
		return x - y;
		break;
	case '*':
		return x * y;
		break;
	case '/':
		return x / y;
		break;
	default:
		printf("Error");
		return 0;
	}
}

int main(void)
{
	int x, y;
	char c;
	scanf("%d %d %c", x, y, c);
}
