#include <stdio.h>

int countHoles(int num)
{
	int temp = num % 10, holes = 0;

	while(num)
	{
		switch(temp)
		{
		case 1:
		case 2:
		case 3:
		case 5:
		case 7:
			break;
		case 0:
		case 4:
		case 6:
		case 9:
			holes ++;
			break;
		case 8:
			holes += 2;
		}
		num /= 10;
		temp = num % 10;
	}
}

