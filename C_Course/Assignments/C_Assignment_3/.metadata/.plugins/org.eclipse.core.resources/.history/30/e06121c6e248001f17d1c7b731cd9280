/*
================================================================
	Name		: q9.c
	Created on	: Jul 23, 2024
	Author		: ameer
	Description	: Prints the nth term in the fibonacci sequence
				  using recursion and using for/while loops
=================================================================
 */
#include <stdio.h>

//Fib Sequence:
// 0,1,1,2,3,5,8.....

unsigned long long fib_sequential(int n)
{
	unsigned long long prev2 = 0, prev1 = 1;
	unsigned long long result = 0;

	for(int i = 3; i <= n; i++)
	{
		result = prev1 + prev2;
		prev2 = prev1;
		prev1 = result;
		printf("%llu\n", result);
	}
	return result;
}

unsigned long long fib_recursive(int n)
{
	if(n == 0 || n == 1)
		return n;
	return fib_recursive(n - 1) + fib_recursive(n - 2);
}

int main(void)
{
	printf("Sequential Method: %llu\n", fib_sequential(100));
	printf("Recursive  Method: %llu", fib_recursive(10));
	return 0;
}


