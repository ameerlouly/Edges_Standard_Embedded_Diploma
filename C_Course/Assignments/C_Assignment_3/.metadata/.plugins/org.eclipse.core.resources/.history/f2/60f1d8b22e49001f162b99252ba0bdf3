#include <stdio.h>

void rmChar(char *c)
{
	int i = 0, j;
	while(c[i])
	{
		j = i;
		if(!((c[i] >= 'A' && c[i] <= 'Z') || (c[i] >= 'a' && c[i] <= 'z')))
		{
			while(c[j])
			{
				c[j] = c[j + 1];
				j++;
			}
		}
		else
			i++;
	}
}

int main(void)
{
	char c[] = "lets try to remove !!!!#$%^&*()&&& those";
	printf("%s\n", c);
	rmChar(c);
	printf("%s\n", c);
}

