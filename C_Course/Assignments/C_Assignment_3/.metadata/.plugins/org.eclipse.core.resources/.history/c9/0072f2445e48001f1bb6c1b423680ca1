#include <stdio.h>
#define SIZE 10

void bubsort(int* arr, int arr_size)
{
	for(int i = 0; i < arr_size; i++)
		for(int j = i; j < arr_size; j++)
			if(arr[i] > arr[j])
			{
				arr[i] ^= arr[j];
				arr[j] ^= arr[i];
				arr[i] ^= arr[j];
			}
}
