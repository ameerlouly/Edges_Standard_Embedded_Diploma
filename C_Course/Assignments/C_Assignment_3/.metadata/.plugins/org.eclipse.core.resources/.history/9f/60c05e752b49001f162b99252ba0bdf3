#include <stdio.h>

int findLength(char *c)
{
	int counter = 0;
	while(c[counter])
		counter++;
	return counter;
}

int main(void)
{
	char c[] = "How many letters are in Embedded C";
	printf("%s\n%d", findLenght(c));
	return 0;;
}




