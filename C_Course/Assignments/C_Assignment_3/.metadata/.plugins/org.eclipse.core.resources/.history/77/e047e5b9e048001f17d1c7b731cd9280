	Name		: q9.c
	Created on	: Jul 23, 2024
	Author		: ameer
	Description	: Prints the nth term in the fibonacci sequence
				  using recursion and using for/while loops
=================================================================
 */
#include <stdio.h>

//Fib Sequence:
// 0,1,1,2,3,5,8.....

unsigned long long fib_sequential(int n)
{
	int prev2 = 0, prev1 = 1;
	unsigned long long result = 0;
	printf("%llu\n", result);

	for(int i = 2; i <= n; i++)
	{
		result = prev1 + prev2;
		prev2 = prev1;
		prev1 = result;
		printf("%llu\n", result);
	}
	return result;
}

int main(void)
{
	printf("Sequential Method: %llu", fib_sequential(50));
	return 0;
}

