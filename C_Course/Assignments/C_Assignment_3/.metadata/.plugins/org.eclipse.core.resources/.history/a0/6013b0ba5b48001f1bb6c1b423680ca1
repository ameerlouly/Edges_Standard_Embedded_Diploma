#include <stdio.h>

int repeatedChar(char *string, int string_size)
{
	for(int i = 0; i < string_size; i++)
		for(int j = i + 1; j < string_size; j++)
			if(string[i] == string[j])
				return 0;
	return 1;
}

int main(void)
{
	char s[7] = "Hello";
	printf("%d", repeatedChar(s, 7));
}

