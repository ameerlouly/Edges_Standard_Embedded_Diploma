#ifndef LINKED_LIST_H_
#define LINKED_LIST_H_

typedef int elemtype;


	/*NODE DEFINITION*/
struct node;
typedef struct node* link;
typedef struct node
{
	int elem;
	link next;
} node;


	/*FUNCTION PROTOTYPES*/
void insertAtFirst(const elemtype e);
int search(const elemtype e);

#endif /* LINKED_LIST_H_ */
