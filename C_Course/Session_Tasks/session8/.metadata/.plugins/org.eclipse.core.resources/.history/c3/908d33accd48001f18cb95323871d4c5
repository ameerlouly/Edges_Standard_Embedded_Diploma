#include <stdio.h>

void print_binary(unsigned int num)
{
	unsigned int mask = 0x80000000; //1000000000000000000... , 1 and all zeros

	for(int i = 0; i < 32; i++)
		(num << i) & mask ? printf("1") : printf("0");
}

int main(void)
{

	return 0;
}
