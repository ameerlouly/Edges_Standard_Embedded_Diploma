int main(void)
{

	insertAtFirst(60);
	insertAtFirst(5);
	insertAtFirst(23);
	insertAtFirst(50);
	insertAtFirst(10);
	insertAtFirst(3);
	insertAtFirst(11);

	printList();

	printf("%d\n", findMiddle());

	int x;
	fifthFromBack(&x);
	printf("%d\n", x);

	insertAndAdd(15, 3);

	printList();

	printf("Middle Element: %d\n", findMiddle());

	fifthFromBack(&x);
		printf("%d\n", x);
}
