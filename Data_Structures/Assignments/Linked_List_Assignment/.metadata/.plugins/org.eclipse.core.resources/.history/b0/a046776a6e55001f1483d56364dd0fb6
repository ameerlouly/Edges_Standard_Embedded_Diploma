#ifndef STACKS_AND_QUEUES_H_
#define STACKS_AND_QUEUES_H_

typedef int elemtype;

struct node;
typedef struct node* link;
typedef struct
{
	elemtype elem;
	link next;
} node;

	/*Stack Functions*/
void push(const elemtype e);
void pop(void);
elemtype peek(void);

#endif /* STACKS_AND_QUEUES_H_ */
