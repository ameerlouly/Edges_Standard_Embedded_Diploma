#include "Stacks_And_Queues.h"
#include <stdio.h>
#include <stdlib.h>
#include <assert.h>

link front = NULL, rear = NULL;

void Enqueue(const elemtype e)
{
	link add = (link)malloc(sizeof(node));
	add->elem = e;

	if(front == NULL)
	{
		front = add;
		rear = add;
		add->next = NULL;
	}

	add->next = front;
	front = add;
}
void Dequeue(void)
{
	link delete_node = rear;
}
elemtype peek_queue(void);
void printList_queue(void);


